{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "pages/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "DATABASE_URL": "@database_url",
    "MYSQL_CA_CERT": "@mysql_ca_cert"
  },
  "regions": ["iad1"],
  "framework": "nextjs"
}
